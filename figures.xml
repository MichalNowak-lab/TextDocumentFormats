<?xml version="1.0" encoding="UTF-8"?>
<geometricFigures>
    <figure>
        <name>Circle</name>
        <type>2D</type>
        <properties>
            <property>Radius</property>
            <property>Diameter</property>
        </properties>
        <formulas>
            <area>π * radius^2</area>
            <perimeter>2 * π * radius</perimeter>
        </formulas>
    </figure>
    <figure>
        <name>Square</name>
        <type>2D</type>
        <properties>
            <property>Side Length</property>
        </properties>
        <formulas>
            <area>side^2</area>
            <perimeter>4 * side</perimeter>
        </formulas>
    </figure>
	<figure>
		<name>Sphere</name>
		<type>3D</type>
		<properties>
            <property>Shape</property>
		    <property>Surface</property>
		    <property>Symmetry</property>
		    <property>Edges</property>
		    <property>Vertices</property>
		    <property>Radius</property>
		    <property>Polyhedron</property>
        </properties>
		<formulas>
			<area>4π*r**2</area>
			<volume>4/3πr**3</volume>
		</formulas>
	</figure>
</geometricFigures>